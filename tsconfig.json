{
  "version": "3.9.3",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "lib": [
      "es6",
      "esnext.asynciterable",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
